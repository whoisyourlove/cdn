    <script>
        var player = videojs(document.querySelector('.video-js'), {
            html5: {
                vhs: {
                    OverrideNative: true,
                },
                nativeAudioTracks: false,
                nativeVideoTracks: false,
            },
            autoplay: true,
            poster: "",
            muted: false,
            controls: true,
            loop: false,
            preload: "auto",
            language: "zh-CN",
            bigPlayButton: false,
            playbackRates: [0.5, 0.8, 1, 1.5, 2, 3, 5],//速率
            controlBar: {
                volumePanel: {
                    inline: false
                },
                playToggle: {
                    replay: false
                }
            }
        });
        player.ready(function () {
            /*//增加沉浸式观影*/
            var myButton = player.controlBar.addChild("button"); //{},13
            myButton.addClass("cqsBtn");
            $(".cqsBtn").html('<i class="bi bi-badge-ar cqsTxt" title="沉浸式观影"></i>');
            myButton.on(['tap', 'click'], function () {
                FullScreen(document.body);
                $(".vjs-control-bar").css("opacity", "0");
                setTimeout(function () { $(".vjs-control-bar").css("opacity", ""); }, 3000);

                $(this).blur();
            });

            //缓冲时增加提示
            $(".vjs-loading-spinner").prepend('<div style="position: fixed;z-index: 999;width: 100%;left: 0;right: 0;margin-top: 52px;text-align: center;"><div class="playerTip">正在请求视频资源</div></div>');

            //隐藏部分控件
            setTimeout(function () {
                onresize2();
            }, 1000);

            //移动端点击播放暂停
            $('video').on('touchstart', function () {
                $(".vjs-play-control").click()
            });
        });
        player.on("play", function () {
        });
        player.on("ended", function () {

        });
        //跳过缓存太久了此片段
        var isPlaying = false;
        player.on('playing', function () {
            isPlaying = true;
            if (waitTimer != undefined) {
                clearTimeout(waitTimer);
                waitTimer = undefined;
            }
        });
        var waitTimer = undefined;
        player.on("waiting", function () {
            isPlaying = false;
            if (waitTimer != undefined) {
                return;
            }
            waitTimer = setTimeout(function () {
                if (!isPlaying && !player.paused()) {
                    console.log("jump waiting");
                    player.currentTime(player.currentTime() + 10);
                }
                waitTimer = undefined;
            }, 8000);
        });
        player.on('error', () => {

        });

        player.on("ratechange", function () {
            //重新设置一下 否则声画不同步
            player.currentTime(player.currentTime() + 0.3);
        });

        player.on("timeupdate", function (a, b) {
            if (chi != 8) {
                chi = 8
            }
        });


        (function () {
            reLoadUrl(decodeURI(getQueryVariable("url")));

        })();
        //////////////////////////////////////////////////////////////////////////////
        $("body,button,i").keydown(function (event) {
            if (['input', 'textarea'].indexOf(event.target.tagName.toLowerCase()) > -1) {
                return;
            }
            var e = event || window.event || arguments.callee.caller.arguments[0];
            if (e && e.keyCode === 38) {
                //上
                player.volume(player.volume() + 0.05);
                return false;
            } else if (e && e.keyCode === 40) {
                //下
                player.volume(player.volume() - 0.05);
                return false;
            } else if (e && e.keyCode === 37) {
                //左
                return false;
            } else if (e && e.keyCode === 39) {
                //右
                player.playbackRate(10);
                return false;
            } else if (e && e.keyCode === 32) {
                //空格
                player.paused() === true ? player.play() : player.pause();
                return false;
            }
        });
        document.onkeyup = function (event) {
            var e = event || window.event || arguments.callee.caller.arguments[0];

            if (e && e.keyCode === 37) {
                //左
                player.currentTime(player.currentTime() - 15);
                return false;
            }

            //放开右
            if (e && e.keyCode === 39) {
                player.playbackRate(1);
            }
        };
        window.oncontextmenu = function (e) {
            e.preventDefault();
        };
        //全屏
        function FullScreen(el) {
            var isFullscreen = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen;
            if (!isFullscreen) { //进入全屏,多重短路表达式
                (el.requestFullscreen && el.requestFullscreen()) ||
                    (el.mozRequestFullScreen && el.mozRequestFullScreen()) ||
                    (el.webkitRequestFullscreen && el.webkitRequestFullscreen()) || (el.msRequestFullscreen && el.msRequestFullscreen());
            } else { //退出全屏,三目运算符
                document.exitFullscreen ? document.exitFullscreen() :
                    document.mozCancelFullScreen ? document.mozCancelFullScreen() :
                        document.webkitExitFullscreen ? document.webkitExitFullscreen() : '';
            }
        }
        //退出全屏
        function ExitFullScreen() {
            try {
                document.exitFullscreen ? document.exitFullscreen() : "";
                document.mozCancelFullScreen ? document.mozCancelFullScreen() : "";
                document.webkitExitFullscreen ? document.webkitExitFullscreen() : "";
            } catch (e) { }
        }

        //退出全屏
        setInterval(function () {
            if (document.isFullScreen || document.mozIsFullScreen || document.webkitIsFullScreen) {
                $(".fullShow").show();
                $(".video-js").addClass("videoFullPage");
            } else {
                $(".fullShow").hide();
                $(".video-js").removeClass("videoFullPage")

            }
        }, 500);

        //手机上隐藏按钮
        function onresize2() {
            if (videojs.browser.IS_ANDROID || videojs.browser.IS_IPHONE) { //($(window).width() < 700) {//字幕按钮
                $(".vjs-subs-caps-button").hide();
                $(".vjs-remaining-time").hide();
                $(".vjs-volume-panel").hide();
                $("#dmSetBtn").hide();
                $(".cqsBtn").hide();

            } else {
                $(".vjs-subs-caps-button").show();
                $(".vjs-remaining-time").show();
                $(".vjs-volume-panel").show();
                $("#dmSetBtn").show();
                $(".cqsBtn").show();
            }
        }
        onresize2();
        window.onresize = function () {
            onresize2();
        };

        function reLoadUrl(url) {
            //初始化提示
            $(".playerTip").html("<div>正在连接服务 大约30秒...</div><div style='margin: 2px;'>如果播放卡顿 可尝试 更换其他视频</div>");
            //video里注册的事件
            onBeginLoadTs = function () {
                try {
                    $(".playerTip").text("正在请求视频资源,即将播放...");
                    //playList = player.tech().hls.playlists.media();
                    chi = 4;
                } catch (e) { }
            };
            onLoadTs = function () { };

            try {
                player.volume(0);

                player.src({
                    src: url
                });
                player.load(url);


                player.play();
                setTimeout(function () {
                    player.volume(100);
                }, 2000);
            } catch (e) {
                // alert("加载视频错误");
            }
        }
    </script>


